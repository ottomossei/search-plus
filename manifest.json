{
  "background": {
    "scripts": [ "js/jquery.min.js"]
 },
  "name": "Search Plus",
  "description": "search_plus is a Google Chrome Extension to help you search rapidly mainly for software engineer.",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
    },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [ "tabs", "storage", "https://*/*", "\u003Call_urls>" ]
}